{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "type": "string",
      "defaultValue": "myUniqueAppName",
      "metadata": {
        "description": "The name of the web app."
      }
    },
    "location": {
      "type": "string",
      "defaultValue": "East US",
      "metadata": {
        "description": "The location where the resources will be deployed."
      }
    },
    "resourceGroup": {
      "type": "string",
      "defaultValue": "myResourceGroup",
      "metadata": {
        "description": "The name of the resource group."
      }
    },
    "hostingPlanName": {
      "type": "string",
      "defaultValue": "myHostingPlan",
      "metadata": {
        "description": "The name of the hosting plan."
      }
    },
    "sku": {
      "type": "string",
      "defaultValue": "S1",
      "metadata": {
        "description": "The SKU of the hosting plan."
      }
    },
    "runtimeStack": {
      "type": "string",
      "defaultValue": "DOTNETCORE|3.1",
      "metadata": {
        "description": "The runtime stack for the web app."
      }
    }
  }
}